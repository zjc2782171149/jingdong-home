AAP;AACF;AAba,CAAf","sourcesContent":["<template>\n  <div class=\"wrapper\">\n    <div class=\"title\">\n      <div\n        class=\"iconfont title__back\"\n        @click=\"handleBackClick\"\n      >&#xe6f2;</div>\n      <div class=\"title__text\">管理收货地址</div>\n      <div class=\"title__add\" @click=\"handleAddClick\">新建</div>\n    </div>\n    <Address\n      v-for=\"address in addressList\"\n      :key=\"address._id\"\n      :address=\"address\"\n      @click=\"() => handleUpdateClick(address._id)\"\n    />\n  </div>\n</template>\n\n<script>\nimport Address from '../../components/Address'\nimport useCommonAddressEffect from '../../effects/addressEffect'\n\nimport { toRefs } from 'vue'\nimport { useStore } from 'vuex'\nimport { useRouter } from 'vue-router'\n\nexport default {\n  name: 'MyAddressList',\n  components: { Address },\n  setup() {\n    const store = useStore()\n    const router = useRouter()\n    const { addressList } = toRefs(store.state)\n    const { getAddressList } = useCommonAddressEffect()\n    getAddressList(true)\n    const handleBackClick = () => { router.back() }\n    const handleAddClick = () => { router.push({ name: 'UpsertAddress'}) }\n    const handleUpdateClick = (addressId)=> {router.push(`/upsertAddress/${addressId}`)}\n    return { addressList, handleBackClick, handleAddClick, handleUpdateClick }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n@import '../../style/viriables.scss';\n.wrapper {\n  overflow-y: auto;\n  position: absolute;\n  left: 0;\n  top: 0;\n  bottom: 0;\n  right: 0;\n  background: $dark-bgColor;\n}\n.title {\n  display: flex;\n  line-height: .44rem;\n  background: $bgColor;\n  font-size: .16rem;\n  color: $content-fontcolor;\n  text-align: center;\n  &__back {\n    width: .2rem;\n    margin-left: .18rem;\n    font-size: .2rem;\n    color: #B6B6B6;\n  }\n  &__text {\n    flex: 1;\n    text-align: center;\n  }\n  &__add {\n    margin-right: .2rem;\n    font-size: .14rem;\n  }\n}\n</style>"],"sourceRoot":""}]}